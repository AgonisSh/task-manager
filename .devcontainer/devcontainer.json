{
    "name": "SecureTask Dev",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "vscjava.vscode-spring-boot-dashboard",
                "vmware.vscode-boot-dev-pack"
            ]
        }
    },
    "postCreateCommand": "mvn dependency:go-offline"
}
